});\n      },[userId,authService,history]);\n\n      const deleteCard = \n        (card) => {\n          console.log(cards.length);\n          // const result = cards.filter(item=>item.id!==card.id);\n          const result = {...cards};\n          delete result[card.id];\n          setCards(result);\n          cardRepository.deleteCard(userId,card);\n        }\n\n\n      const createOrUpdateCard = \n        (card) => {\n          setCards(cards=>{\n            const update = {...cards};\n            update[card.id] = card;\n            return update;\n          });\n          cardRepository.saveCard(userId,card);\n        }\n      \n      \n\n    return (<section className={styles.maker}>\n        <Header onLogout={onLogout} />\n            <section className={styles.sectionBox}>\n                <Editer cards={cards}\n                onDelete={deleteCard}\n                onAdd={createOrUpdateCard}\n                onUpdate={createOrUpdateCard}\n                FileInput={FileInput}\n                />\n                <Preview cards={cards}/>\n            </section>\n        <Footer/>\n        </section>\n    );\n\n});\n\nexport default Maker;\n","/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/preview/preview.jsx",[],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/editer/editer.jsx",[],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/cards/card.jsx",["90"],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/cardEditForm/cardEditForm.jsx",["91"],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/button/button.jsx",[],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/imageFileInput/imageFileInput.jsx",[],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/components/cardAddForm/cardAddForm.jsx",[],"/Users/kimteahyeong/Desktop/Web/react-basic/card-maker/src/service/imageUploader.js",[],"/Users/kimteahyeong/Desktop/Web/react-basic/ca